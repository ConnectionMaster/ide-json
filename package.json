{
  "name": "ide-json",
  "main": "./build/main",
  "version": "0.1.0",
  "description": "JSON language support for Atom-IDE.",
  "repository": "rwatts3/atom-ide-json",
  "license": "MIT",
  "engines": {
    "atom": ">=1.19.0"
  },
  "keywords": [
    "atom-ide",
    "json",
    "lsp",
    "language-client"
  ],
  "author": {
    "name": "Ryan Watts",
    "url": "https://github.com/rwatts3",
    "email": "ryandwatts@gmail.com"
  },
  "scripts": {
    "dev": "tsc --watch",
    "compile": "tsc",
    "prepublish": "npm run compile"
  },
  "dependencies": {
    "atom-languageclient": "^0.4.0",
    "fuzzaldrin-plus": "^0.4.1",
    "vscode-json-languageserver-bin": "latest"
  },
  "devDependencies": {
    "typescript": "^2.4.2",
    "@types/node": "latest"
  },
  "enhancedScopes": [
    "source.json"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
